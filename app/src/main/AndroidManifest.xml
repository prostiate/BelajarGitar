<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vincent.belajargitar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AboutMeActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".CkunciActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".DkunciActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".AkunciActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".BkunciActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".EkunciActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".GkunciActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".FkunciActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".PilihkunciActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>